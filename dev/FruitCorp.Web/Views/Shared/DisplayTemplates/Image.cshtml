@model EPiServer.Url

@if (Model != null && !Model.IsEmpty()){
    string width = string.Empty, imgclass = string.Empty, dsource = string.Empty;
    if(ViewData.ContainsKey("width")){
        width = ViewData["width"].ToString();
    }
    
    if (ViewData.ContainsKey("imgclass"))
    {
        imgclass = ViewData["imgclass"].ToString();
    }
    
    if (ViewData.ContainsKey("dsource"))
    {
        dsource = ViewData["dsource"].ToString();
    }
    <img src="@Model" width="@width" class="@imgclass" data-source="@dsource" />
}